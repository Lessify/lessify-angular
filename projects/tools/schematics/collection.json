{
  "$schema": "../../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "i18n-merge": {
      "description": "Merge i18n json files.",
      "factory": "./i18n-merge/index"
    },
    "i18n-export-csv": {
      "description": "Export all i18n json files into CSV.",
      "factory": "./i18n-export-csv/index"
    },
    "i18n-import-csv": {
      "description": "Import CSV file into all i18n json files.",
      "factory": "./i18n-import-csv/index"
    }
  }
}
